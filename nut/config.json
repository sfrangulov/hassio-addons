{
  "name": "Network UPS Tools",
  "version": "0.3",
  "slug": "nut",
  "description": "Manage Uninterruptible Power Supplies",
  "arch": [
    "amd64",
    "armhf"
  ],
  "startup": "before",
  "devices": ["/dev/ttyUSB0:/dev/ttyUSB0:rwm"],
  "ports": {
    "3493/tcp": 3493
  },
  "boot": "auto",
  "options": {
    "logins": [
      { "username": "monuser", "password": "secret", "instcmds": "all" }
    ],
    "ups": [
      { "upsname": "ups", "driver": "blazer_ser", "port": "/dev/ttyUSB0" }
    ],
    "bindaddr": "0.0.0.0",
    "bindport": "3493",
    "mode": "netserver"
  },
  "schema": {
    "logins": [
      { "username": "str", "password": "str", "instcmds": "str" }
    ],
    "ups": [
      { "upsname": "str", "driver": "str", "port": "str" }
    ],
    "bindaddr": "str",
    "bindport": "port",
    "mode": "str"
  }
}
